  "version": "0.2.0",
  "configurations": [
    {
        "type": "node",
        "request": "launch",
        "name": "Debug Jest Tests",
        "program": "${workspaceFolder}/node_modules/.bin/jest.js",
        "args": ["--runInBand", "${workspaceFolder}/src/__tests__"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "启动程序11",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\examples\\webpack.config.js",
      "outFiles": ["${workspaceFolder}/server.js"]
    }
  ]
}
